{
  "components": [
    {
      "type": "content",
      "container": "main",
      "components": [
        {
          "type": "row",
          "components": [
            {
              "type": "col",
              "class": "col-md-12",
              "components": [
                {
                  "type": "template-form",
                  "name": "form0",
                  "components": [
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Opis",
                              "component": "Opis",
                              "name": "OpisLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "string",
                              "maxLength": 50,
                              "type": "textbox",
                              "name": "Opis",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${tblvaluti.Opis}"
                            },
                            {
                              "type": "required-validator",
                              "name": "OpisRequiredValidator",
                              "component": "Opis",
                              "text": "Opis is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Kratenka",
                              "component": "Kratenka",
                              "name": "KratenkaLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "string",
                              "maxLength": 3,
                              "type": "textbox",
                              "name": "Kratenka",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${tblvaluti.Kratenka}"
                            },
                            {
                              "type": "required-validator",
                              "name": "KratenkaRequiredValidator",
                              "component": "Kratenka",
                              "text": "Kratenka is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "components": [
                        {
                          "type": "col",
                          "class": "col offset-sm-3",
                          "components": [
                            {
                              "type": "button",
                              "buttonType": "submit",
                              "icon": "save",
                              "text": "Save",
                              "buttonStyle": "primary"
                            },
                            {
                              "type": "button",
                              "buttonStyle": "light",
                              "style": {
                                "marginLeft": "1rem"
                              },
                              "text": "Cancel",
                              "events": {
                                "click": [
                                  {
                                    "type": "close-popup"
                                  }
                                ]
                              }
                            }
                          ]
                        }
                      ]
                    }
                  ],
                  "events": {
                    "submit": [
                      {
                        "error": [
                          {
                            "detail": "Unable to update TblValuti",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "BSSR4.updateTblValuti",
                        "parameters": [
                          {
                            "name": "ID",
                            "value": "${parameters.ID}"
                          },
                          {
                            "name": "TblValuti",
                            "value": "${tblvaluti}"
                          }
                        ],
                        "then": [
                          {
                            "type": "close-popup",
                            "result": "${tblvaluti}"
                          }
                        ],
                        "type": "invoke"
                      }
                    ]
                  },
                  "itemType": "Bssr.Models.Bssr4.TblValuti",
                  "data": "${tblvaluti}",
                  "visible": "${tblvaluti != null}"
                }
              ]
            }
          ]
        }
      ]
    }
  ],
  "events": {
    "load": [
      {
        "name": "BSSR4.getTblValutiByID",
        "parameters": [
          {
            "name": "ID",
            "value": "${parameters.ID}"
          }
        ],
        "then": [
          {
            "name": "tblvaluti",
            "type": "set",
            "value": "${result}",
            "valueType": "Bssr.Models.Bssr4.TblValuti"
          }
        ],
        "error": [],
        "type": "invoke"
      }
    ]
  },
  "name": "Edit Tbl Valuti",
  "layout": "Main"
}