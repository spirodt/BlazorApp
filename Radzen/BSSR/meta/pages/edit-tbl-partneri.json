  "events": {
    "load": [
      {
        "name": "BSSR4.getTblPartneriByID",
        "parameters": [
          {
            "name": "ID",
            "value": "${parameters.ID}"
          }
        ],
        "then": [
          {
            "name": "tblpartneri",
            "type": "set",
            "value": "${result}",
            "valueType": "Bssr.Models.Bssr4.TblPartneri"
          }
        ],
        "error": [],
