                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "string",
                              "maxLength": 50,
                              "type": "textbox",
                              "name": "Ime",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${tblconfig.Ime}"
                            },
                            {
                              "type": "required-validator",
                              "name": "ImeRequiredValidator",
                              "component": "Ime",
                              "text": "Ime is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Vrednost",
                              "component": "Vrednost",
                              "name": "VrednostLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "int?",
                              "type": "numeric",
                              "name": "Vrednost",
                              "style": {
                                "width": "100%"
                              },
                              "value": "${tblconfig.Vrednost}"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Nivo Na Administracija",
                              "component": "NivoNaAdministracija",
                              "name": "NivoNaAdministracijaLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "int?",
                              "type": "numeric",
                              "name": "NivoNaAdministracija",
                              "style": {
                                "width": "100%"
                              },
                              "value": "${tblconfig.NivoNaAdministracija}"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Range",
                              "component": "Range",
                              "name": "RangeLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "string",
                              "maxLength": 10,
                              "type": "textbox",
                              "name": "Range",
                              "style": {
                                "width": "100%"
                              },
                              "value": "${tblconfig.Range}"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Ime Na Modul",
                              "component": "ImeNaModul",
                              "name": "ImeNaModulLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "string",
                              "maxLength": 50,
                              "type": "textbox",
                              "name": "ImeNaModul",
                              "style": {
                                "width": "100%"
                              },
                              "value": "${tblconfig.ImeNaModul}"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Firma Id",
                              "component": "FirmaID",
                              "name": "FirmaIDLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "int",
                              "type": "numeric",
                              "name": "FirmaID",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${tblconfig.FirmaID}"
                            },
                            {
                              "type": "required-validator",
                              "name": "FirmaIDRequiredValidator",
                              "component": "FirmaID",
                              "text": "FirmaID is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "components": [
                        {
                          "type": "col",
                          "class": "col offset-sm-3",
                          "components": [
                            {
                              "type": "button",
                              "buttonType": "submit",
                              "icon": "save",
                              "text": "Save",
                              "buttonStyle": "primary"
                            },
                            {
                              "type": "button",
                              "buttonStyle": "light",
                              "style": {
                                "marginLeft": "1rem"
                              },
                              "text": "Cancel",
                              "events": {
                                "click": [
                                  {
                                    "type": "close-popup"
                                  }
                                ]
                              }
                            }
                          ]
                        }
                      ]
                    }
                  ],
                  "events": {
                    "submit": [
                      {
                        "error": [
                          {
                            "detail": "Unable to create new TblConfig!",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "BSSR4.createTblConfig",
                        "parameters": [
                          {
                            "name": "TblConfig",
                            "value": "${tblconfig}"
                          }
                        ],
                        "then": [
                          {
                            "type": "close-popup",
                            "result": "${tblconfig}"
                          }
                        ],
                        "type": "invoke"
                      }
                    ]
                  },
                  "data": "${tblconfig}",
                  "visible": "${tblconfig != null}",
                  "itemType": "Bssr.Models.Bssr4.TblConfig"
                }
              ]
            }
          ]
        }
      ]
    }
  ],
  "events": {
    "load": [
      {
        "name": "tblconfig",
        "type": "set",
        "value": "new Bssr.Models.Bssr4.TblConfig(){}",
        "valueType": "Bssr.Models.Bssr4.TblConfig"
      }
    ]
  },
  "name": "Add Tbl Config",
  "layout": "Main",
  "access": []
}