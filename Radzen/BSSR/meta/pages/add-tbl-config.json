                  "components": [
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Opis",
                              "component": "Opis",
                              "name": "OpisLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "string",
                              "maxLength": 50,
                              "type": "textbox",
                              "name": "Opis",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${tblconfig.Opis}"
                            },
                            {
                              "type": "required-validator",
                              "name": "OpisRequiredValidator",
                              "component": "Opis",
                              "text": "Opis is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
