{
  "components": [
    {
      "type": "content",
      "container": "main",
      "components": [
        {
          "type": "row",
          "components": [
            {
              "type": "col",
              "class": "col-md-12",
              "components": [
                {
                  "type": "template-form",
                  "name": "form0",
                  "components": [
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Tbl Valuta Id",
                              "component": "tblValutaID",
                              "name": "tblValutaIDLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "int?",
                              "type": "numeric",
                              "name": "tblValutaID",
                              "style": {
                                "width": "100%"
                              },
                              "value": "${tblkursnalistum.tblValutaID}"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Kurs",
                              "component": "Kurs",
                              "name": "KursLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "decimal?",
                              "type": "numeric",
                              "name": "Kurs",
                              "style": {
                                "width": "100%"
                              },
                              "value": "${tblkursnalistum.Kurs}"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "components": [
                        {
                          "type": "col",
                          "class": "col offset-sm-3",
                          "components": [
                            {
                              "type": "button",
                              "buttonType": "submit",
                              "icon": "save",
                              "text": "Save",
                              "buttonStyle": "primary"
                            },
                            {
                              "type": "button",
                              "buttonStyle": "light",
                              "style": {
                                "marginLeft": "1rem"
                              },
                              "text": "Cancel",
                              "events": {
                                "click": [
                                  {
                                    "type": "close-popup"
                                  }
                                ]
                              }
                            }
                          ]
                        }
                      ]
                    }
                  ],
                  "events": {
                    "submit": [
                      {
                        "error": [
                          {
                            "detail": "Unable to create new TblKursnaListum!",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "BSSR4.createTblKursnaListum",
                        "parameters": [
                          {
                            "name": "TblKursnaListum",
                            "value": "${tblkursnalistum}"
                          }
                        ],
                        "then": [
                          {
                            "type": "close-popup",
                            "result": "${tblkursnalistum}"
                          }
                        ],
                        "type": "invoke"
                      }
                    ]
                  },
                  "data": "${tblkursnalistum}",
                  "visible": "${tblkursnalistum != null}",
                  "itemType": "Bssr.Models.Bssr4.TblKursnaListum"
                }
              ]
            }
          ]
        }
      ]
    }
  ],
  "events": {
    "load": [
      {
        "name": "tblkursnalistum",
        "type": "set",
        "value": "new Bssr.Models.Bssr4.TblKursnaListum(){}",
        "valueType": "Bssr.Models.Bssr4.TblKursnaListum"
      }
    ]
  },
  "name": "Add Tbl Kursna Lista",
  "layout": "Main"
}