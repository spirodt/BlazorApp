{
  "components": [
    {
      "type": "content",
      "container": "main",
      "components": [
        {
          "type": "row",
          "components": [
            {
              "type": "col",
              "class": "col-md-12",
              "components": [
                {
                  "type": "template-form",
                  "name": "form0",
                  "components": [
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Tbl Normativ",
                              "component": "tblNormativID",
                              "name": "tblNormativIDLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "data": "${getTblNormativsFortblNormativIDResult}",
                              "textProperty": "Naziv",
                              "valueProperty": "ID",
                              "placeholder": "Choose TblNormativ",
                              "valueType": "int",
                              "showSearch": false,
                              "type": "dropdown-datagrid",
                              "name": "tblNormativID",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${tblnormativstavki.tblNormativID}"
                            },
                            {
                              "type": "required-validator",
                              "name": "tblNormativIDRequiredValidator",
                              "component": "tblNormativID",
                              "text": "tblNormativID is required",
                              "style": {
                                "position": "absolute"
                              },
                              "defaultValue": "0"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Tbl Artikal",
                              "component": "tblArtikalID",
                              "name": "tblArtikalIDLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "data": "${getTblArtikalsFortblArtikalIDResult}",
                              "textProperty": "Sifra",
                              "valueProperty": "ID",
                              "placeholder": "Choose TblArtikal",
                              "valueType": "int",
                              "showSearch": false,
                              "type": "dropdown-datagrid",
                              "name": "tblArtikalID",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${tblnormativstavki.tblArtikalID}"
                            },
                            {
                              "type": "required-validator",
                              "name": "tblArtikalIDRequiredValidator",
                              "component": "tblArtikalID",
                              "text": "tblArtikalID is required",
                              "style": {
                                "position": "absolute"
                              },
                              "defaultValue": "0"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Status",
                              "component": "Status",
                              "name": "StatusLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "int?",
                              "type": "numeric",
                              "name": "Status",
                              "style": {
                                "width": "100%"
                              },
                              "value": "${tblnormativstavki.Status}"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Kolicina",
                              "component": "Kolicina",
                              "name": "KolicinaLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "decimal",
                              "type": "numeric",
                              "name": "Kolicina",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${tblnormativstavki.Kolicina}"
                            },
                            {
                              "type": "required-validator",
                              "name": "KolicinaRequiredValidator",
                              "component": "Kolicina",
                              "text": "Kolicina is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "components": [
                        {
                          "type": "col",
                          "class": "col offset-sm-3",
                          "components": [
                            {
                              "type": "button",
                              "buttonType": "submit",
                              "icon": "save",
                              "text": "Save",
                              "buttonStyle": "primary"
                            },
                            {
                              "type": "button",
                              "buttonStyle": "light",
                              "style": {
                                "marginLeft": "1rem"
                              },
                              "text": "Cancel",
                              "events": {
                                "click": [
                                  {
                                    "type": "close-popup"
                                  }
                                ]
                              }
                            }
                          ]
                        }
                      ]
                    }
                  ],
                  "events": {
                    "submit": [
                      {
                        "error": [
                          {
                            "detail": "Unable to update TblNormativStavki",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "BSSR4.updateTblNormativStavki",
                        "parameters": [
                          {
                            "name": "ID",
                            "value": "${parameters.ID}"
                          },
                          {
                            "name": "TblNormativStavki",
                            "value": "${tblnormativstavki}"
                          }
                        ],
                        "then": [
                          {
                            "type": "close-popup",
                            "result": "${tblnormativstavki}"
                          }
                        ],
                        "type": "invoke"
                      }
                    ]
                  },
                  "itemType": "Bssr.Models.Bssr4.TblNormativStavki",
                  "data": "${tblnormativstavki}",
                  "visible": "${tblnormativstavki != null}"
                }
              ]
            }
          ]
        }
      ]
    }
  ],
  "events": {
    "load": [
      {
        "name": "BSSR4.getTblNormativStavkiByID",
        "parameters": [
          {
            "name": "ID",
            "value": "${parameters.ID}"
          }
        ],
        "then": [
          {
            "name": "tblnormativstavki",
            "type": "set",
            "value": "${result}",
            "valueType": "Bssr.Models.Bssr4.TblNormativStavki"
          }
        ],
        "error": [],
        "type": "invoke"
      },
      {
        "name": "BSSR4.getTblNormativs",
        "parameters": [],
        "then": [
          {
            "name": "getTblNormativsFortblNormativIDResult",
            "type": "set",
            "value": "${result}",
            "valueType": "IEnumerable<Bssr.Models.Bssr4.TblNormativ>"
          }
        ],
        "type": "invoke"
      },
      {
        "name": "BSSR4.getTblArtikals",
        "parameters": [],
        "then": [
          {
            "name": "getTblArtikalsFortblArtikalIDResult",
            "type": "set",
            "value": "${result}",
            "valueType": "IEnumerable<Bssr.Models.Bssr4.TblArtikal>"
          }
        ],
        "type": "invoke"
      }
    ]
  },
  "name": "Edit Tbl Normativ Stavki",
  "layout": "Main"
}