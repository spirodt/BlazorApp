{
  "components": [
    {
      "type": "content",
      "container": "main",
      "components": [
        {
          "type": "row",
          "components": [
            {
              "type": "col",
              "class": "col-md-12",
              "components": [
                {
                  "type": "template-form",
                  "name": "form0",
                  "components": [
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Tbl User",
                              "component": "UserID",
                              "name": "UserIDLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "data": "${getTblUsersForUserIDResult}",
                              "textProperty": "UserName",
                              "valueProperty": "ID",
                              "placeholder": "Choose TblUser",
                              "valueType": "int",
                              "showSearch": false,
                              "type": "dropdown-datagrid",
                              "name": "UserID",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${tbluserconfig.UserID}"
                            },
                            {
                              "type": "required-validator",
                              "name": "UserIDRequiredValidator",
                              "component": "UserID",
                              "text": "UserID is required",
                              "style": {
                                "position": "absolute"
                              },
                              "defaultValue": "0"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Object Name",
                              "component": "ObjectName",
                              "name": "ObjectNameLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "string",
                              "maxLength": 300,
                              "type": "textbox",
                              "name": "ObjectName",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${tbluserconfig.ObjectName}"
                            },
                            {
                              "type": "required-validator",
                              "name": "ObjectNameRequiredValidator",
                              "component": "ObjectName",
                              "text": "ObjectName is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Config Data",
                              "component": "ConfigData",
                              "name": "ConfigDataLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "string",
                              "type": "textbox",
                              "name": "ConfigData",
                              "style": {
                                "width": "100%"
                              },
                              "value": "${tbluserconfig.ConfigData}"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Date Of Creation",
                              "component": "DateOfCreation",
                              "name": "DateOfCreationLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "DateTime",
                              "dateFormat": "MM/dd/yyyy",
                              "type": "datepicker",
                              "name": "DateOfCreation",
                              "style": {
                                "width": "100%"
                              },
                              "value": "${tbluserconfig.DateOfCreation}"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "components": [
                        {
                          "type": "col",
                          "class": "col offset-sm-3",
                          "components": [
                            {
                              "type": "button",
                              "buttonType": "submit",
                              "icon": "save",
                              "text": "Save",
                              "buttonStyle": "primary"
                            },
                            {
                              "type": "button",
                              "buttonStyle": "light",
                              "style": {
                                "marginLeft": "1rem"
                              },
                              "text": "Cancel",
                              "events": {
                                "click": [
                                  {
                                    "type": "close-popup"
                                  }
                                ]
                              }
                            }
                          ]
                        }
                      ]
                    }
                  ],
                  "events": {
                    "submit": [
                      {
                        "error": [
                          {
                            "detail": "Unable to create new TblUserConfig!",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "BSSR4.createTblUserConfig",
                        "parameters": [
                          {
                            "name": "TblUserConfig",
                            "value": "${tbluserconfig}"
                          }
                        ],
                        "then": [
                          {
                            "type": "close-popup",
                            "result": "${tbluserconfig}"
                          }
                        ],
                        "type": "invoke"
                      }
                    ]
                  },
                  "data": "${tbluserconfig}",
                  "visible": "${tbluserconfig != null}",
                  "itemType": "Bssr.Models.Bssr4.TblUserConfig"
                }
              ]
            }
          ]
        }
      ]
    }
  ],
  "events": {
    "load": [
      {
        "name": "BSSR4.getTblUsers",
        "parameters": [],
        "then": [
          {
            "name": "getTblUsersForUserIDResult",
            "type": "set",
            "value": "${result}",
            "valueType": "IEnumerable<Bssr.Models.Bssr4.TblUser>"
          }
        ],
        "type": "invoke"
      },
      {
        "name": "tbluserconfig",
        "type": "set",
        "value": "new Bssr.Models.Bssr4.TblUserConfig(){}",
        "valueType": "Bssr.Models.Bssr4.TblUserConfig"
      }
    ]
  },
  "name": "Add Tbl User Configs",
  "layout": "Main",
  "access": []
}