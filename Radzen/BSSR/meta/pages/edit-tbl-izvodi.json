{
  "components": [
    {
      "type": "content",
      "container": "main",
      "components": [
        {
          "type": "row",
          "components": [
            {
              "type": "col",
              "class": "col-md-12",
              "components": [
                {
                  "type": "template-form",
                  "name": "form0",
                  "components": [
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Broj Na Izvod",
                              "component": "BrojNaIzvod",
                              "name": "BrojNaIzvodLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "string",
                              "maxLength": 50,
                              "type": "textbox",
                              "name": "BrojNaIzvod",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${tblizvodi.BrojNaIzvod}"
                            },
                            {
                              "type": "required-validator",
                              "name": "BrojNaIzvodRequiredValidator",
                              "component": "BrojNaIzvod",
                              "text": "BrojNaIzvod is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Zabeleska",
                              "component": "Zabeleska",
                              "name": "ZabeleskaLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "string",
                              "maxLength": 250,
                              "type": "textbox",
                              "name": "Zabeleska",
                              "style": {
                                "width": "100%"
                              },
                              "value": "${tblizvodi.Zabeleska}"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Smetka",
                              "component": "Smetka",
                              "name": "SmetkaLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "string",
                              "maxLength": 50,
                              "type": "textbox",
                              "name": "Smetka",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${tblizvodi.Smetka}"
                            },
                            {
                              "type": "required-validator",
                              "name": "SmetkaRequiredValidator",
                              "component": "Smetka",
                              "text": "Smetka is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Datum Na Izvod",
                              "component": "DatumNaIzvod",
                              "name": "DatumNaIzvodLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "DateTime?",
                              "dateFormat": "MM/dd/yyyy",
                              "type": "datepicker",
                              "name": "DatumNaIzvod",
                              "style": {
                                "width": "100%"
                              },
                              "value": "${tblizvodi.DatumNaIzvod}"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "components": [
                        {
                          "type": "col",
                          "class": "col offset-sm-3",
                          "components": [
                            {
                              "type": "button",
                              "buttonType": "submit",
                              "icon": "save",
                              "text": "Save",
                              "buttonStyle": "primary"
                            },
                            {
                              "type": "button",
                              "buttonStyle": "light",
                              "style": {
                                "marginLeft": "1rem"
                              },
                              "text": "Cancel",
                              "events": {
                                "click": [
                                  {
                                    "type": "close-popup"
                                  }
                                ]
                              }
                            }
                          ]
                        }
                      ]
                    }
                  ],
                  "events": {
                    "submit": [
                      {
                        "error": [
                          {
                            "detail": "Unable to update TblIzvodi",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "BSSR4.updateTblIzvodi",
                        "parameters": [
                          {
                            "name": "ID",
                            "value": "${parameters.ID}"
                          },
                          {
                            "name": "TblIzvodi",
                            "value": "${tblizvodi}"
                          }
                        ],
                        "then": [
                          {
                            "type": "close-popup",
                            "result": "${tblizvodi}"
                          }
                        ],
                        "type": "invoke"
                      }
                    ]
                  },
                  "itemType": "Bssr.Models.Bssr4.TblIzvodi",
                  "data": "${tblizvodi}",
                  "visible": "${tblizvodi != null}"
                }
              ]
            }
          ]
        }
      ]
    }
  ],
  "events": {
    "load": [
      {
        "name": "BSSR4.getTblIzvodiByID",
        "parameters": [
          {
            "name": "ID",
            "value": "${parameters.ID}"
          }
        ],
        "then": [
          {
            "name": "tblizvodi",
            "type": "set",
            "value": "${result}",
            "valueType": "Bssr.Models.Bssr4.TblIzvodi"
          }
        ],
        "error": [],
        "type": "invoke"
      }
    ]
  },
  "name": "Edit Tbl Izvodi",
  "layout": "Main"
}