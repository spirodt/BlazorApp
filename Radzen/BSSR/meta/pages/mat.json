{
  "components": [
    {
      "type": "content",
      "container": "main",
      "components": [
        {
          "type": "heading",
          "name": "pageTitle",
          "size": "H1",
          "text": "Mat"
        },
        {
          "type": "row",
          "components": [
            {
              "type": "col",
              "class": "col-md-12",
              "components": [
                {
                  "events": {
                    "click": [
                      {
                        "condition": "${event?.Value == \"csv\"}",
                        "fileName": "Mat",
                        "name": "BSSR4.getMats",
                        "parameters": [
                          {
                            "name": "$filter",
                            "value": "${grid0.Query.Filter}"
                          },
                          {
                            "name": "$orderby",
                            "value": "${grid0.Query.OrderBy}"
                          }
                        ],
                        "type": "export"
                      },
                      {
                        "condition": "${event?.Value == \"xlsx\"}",
                        "fileName": "Mat",
                        "format": "xlsx",
                        "name": "BSSR4.getMats",
                        "parameters": [
                          {
                            "name": "$filter",
                            "value": "${grid0.Query.Filter}"
                          },
                          {
                            "name": "$orderby",
                            "value": "${grid0.Query.OrderBy}"
                          }
                        ],
                        "type": "export"
                      }
                    ]
                  },
                  "icon": "get_app",
                  "items": [
                    {
                      "icon": "",
                      "text": "Excel",
                      "value": "xlsx"
                    },
                    {
                      "icon": "",
                      "text": "CSV",
                      "value": "csv"
                    }
                  ],
                  "name": "splitbutton0",
                  "style": {
                    "marginLeft": "0px",
                    "marginBottom": "10px"
                  },
                  "text": "Export",
                  "type": "splitbutton"
                },
                {
                  "attributes": [
                    {
                      "name": "@oninput",
                      "value": "@(async(args) => {search = $\"{args.Value}\";await Load();})"
                    }
                  ],
                  "name": "textbox0",
                  "placeholder": "Search ...",
                  "style": {
                    "display": "block",
                    "marginBottom": "10px",
                    "width": "100%"
                  },
                  "type": "textbox",
                  "value": ""
                },
                {
                  "name": "grid0",
                  "allowFiltering": true,
                  "filterMode": "advanced",
                  "allowPaging": true,
                  "allowSorting": true,
                  "data": "${getMatsResult}",
                  "events": {
                    "loadData": []
                  },
                  "title": "",
                  "columns": [
                    {
                      "property": "Naziv",
                      "title": "Naziv"
                    },
                    {
                      "property": "Kolicina",
                      "title": "Kolicina"
                    },
                    {
                      "property": "Iznos",
                      "title": "Iznos"
                    },
                    {
                      "property": "ProdaznaCena",
                      "title": "Prodazna Cena"
                    },
                    {
                      "property": "Sifra",
                      "title": "Sifra"
                    },
                    {
                      "property": "NalogID",
                      "title": "Nalog ID"
                    },
                    {
                      "property": "Artikal",
                      "title": "Artikal"
                    },
                    {
                      "property": "Tip",
                      "title": "Tip"
                    }
                  ],
                  "type": "grid",
                  "itemType": "Bssr.Models.Bssr4.Mat",
                  "pageSize": 20
                }
              ]
            }
          ]
        }
      ]
    }
  ],
  "events": {
    "load": [
      {
        "condition": "string.IsNullOrEmpty(search)",
        "name": "search",
        "type": "set",
        "value": "\"\"",
        "valueType": "string"
      },
      {
        "name": "BSSR4.getMats",
        "parameters": [
          {
            "name": "$filter",
            "operator": "or",
            "value": [
              {
                "operation": "contains",
                "property": "Naziv",
                "type": "string",
                "value": "\"${search}\""
              },
              {
                "operation": "contains",
                "property": "Sifra",
                "type": "string",
                "value": "\"${search}\""
              }
            ]
          }
        ],
        "then": [
          {
            "name": "getMatsResult",
            "type": "set",
            "value": "${result}",
            "valueType": "IEnumerable<Bssr.Models.Bssr4.Mat>"
          }
        ],
        "type": "invoke"
      }
    ]
  },
  "name": "Mat",
  "layout": "Main"
}