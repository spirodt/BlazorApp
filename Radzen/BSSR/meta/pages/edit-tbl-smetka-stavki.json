{
  "components": [
    {
      "type": "content",
      "container": "main",
      "components": [
        {
          "type": "row",
          "components": [
            {
              "type": "col",
              "class": "col-md-12",
              "components": [
                {
                  "type": "template-form",
                  "name": "form0",
                  "components": [
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Tbl Smetka",
                              "component": "tblSmetkaID",
                              "name": "tblSmetkaIDLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "data": "${getTblSmetkasFortblSmetkaIDResult}",
                              "textProperty": "Zabeleska",
                              "valueProperty": "ID",
                              "placeholder": "Choose TblSmetka",
                              "valueType": "int",
                              "showSearch": false,
                              "type": "dropdown-datagrid",
                              "name": "tblSmetkaID",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${tblsmetkastavki.tblSmetkaID}"
                            },
                            {
                              "type": "required-validator",
                              "name": "tblSmetkaIDRequiredValidator",
                              "component": "tblSmetkaID",
                              "text": "tblSmetkaID is required",
                              "style": {
                                "position": "absolute"
                              },
                              "defaultValue": "0"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Tbl Nalog",
                              "component": "tblNalogID",
                              "name": "tblNalogIDLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "data": "${getTblNalogsFortblNalogIDResult}",
                              "textProperty": "Zabeleska",
                              "valueProperty": "ID",
                              "placeholder": "Choose TblNalog",
                              "valueType": "int",
                              "showSearch": false,
                              "type": "dropdown-datagrid",
                              "name": "tblNalogID",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${tblsmetkastavki.tblNalogID}"
                            },
                            {
                              "type": "required-validator",
                              "name": "tblNalogIDRequiredValidator",
                              "component": "tblNalogID",
                              "text": "tblNalogID is required",
                              "style": {
                                "position": "absolute"
                              },
                              "defaultValue": "0"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Status",
                              "component": "Status",
                              "name": "StatusLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "int?",
                              "type": "numeric",
                              "name": "Status",
                              "style": {
                                "width": "100%"
                              },
                              "value": "${tblsmetkastavki.Status}"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Kolicina",
                              "component": "Kolicina",
                              "name": "KolicinaLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "int",
                              "type": "numeric",
                              "name": "Kolicina",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${tblsmetkastavki.Kolicina}"
                            },
                            {
                              "type": "required-validator",
                              "name": "KolicinaRequiredValidator",
                              "component": "Kolicina",
                              "text": "Kolicina is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Cena",
                              "component": "Cena",
                              "name": "CenaLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "decimal",
                              "type": "numeric",
                              "name": "Cena",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${tblsmetkastavki.Cena}"
                            },
                            {
                              "type": "required-validator",
                              "name": "CenaRequiredValidator",
                              "component": "Cena",
                              "text": "Cena is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "components": [
                        {
                          "type": "col",
                          "class": "col offset-sm-3",
                          "components": [
                            {
                              "type": "button",
                              "buttonType": "submit",
                              "icon": "save",
                              "text": "Save",
                              "buttonStyle": "primary"
                            },
                            {
                              "type": "button",
                              "buttonStyle": "light",
                              "style": {
                                "marginLeft": "1rem"
                              },
                              "text": "Cancel",
                              "events": {
                                "click": [
                                  {
                                    "type": "close-popup"
                                  }
                                ]
                              }
                            }
                          ]
                        }
                      ]
                    }
                  ],
                  "events": {
                    "submit": [
                      {
                        "error": [
                          {
                            "detail": "Unable to update TblSmetkaStavki",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "BSSR4.updateTblSmetkaStavki",
                        "parameters": [
                          {
                            "name": "ID",
                            "value": "${parameters.ID}"
                          },
                          {
                            "name": "TblSmetkaStavki",
                            "value": "${tblsmetkastavki}"
                          }
                        ],
                        "then": [
                          {
                            "type": "close-popup",
                            "result": "${tblsmetkastavki}"
                          }
                        ],
                        "type": "invoke"
                      }
                    ]
                  },
                  "itemType": "Bssr.Models.Bssr4.TblSmetkaStavki",
                  "data": "${tblsmetkastavki}",
                  "visible": "${tblsmetkastavki != null}"
                }
              ]
            }
          ]
        }
      ]
    }
  ],
  "events": {
    "load": [
      {
        "name": "BSSR4.getTblSmetkaStavkiByID",
        "parameters": [
          {
            "name": "ID",
            "value": "${parameters.ID}"
          }
        ],
        "then": [
          {
            "name": "tblsmetkastavki",
            "type": "set",
            "value": "${result}",
            "valueType": "Bssr.Models.Bssr4.TblSmetkaStavki"
          }
        ],
        "error": [],
        "type": "invoke"
      },
      {
        "name": "BSSR4.getTblSmetkas",
        "parameters": [],
        "then": [
          {
            "name": "getTblSmetkasFortblSmetkaIDResult",
            "type": "set",
            "value": "${result}",
            "valueType": "IEnumerable<Bssr.Models.Bssr4.TblSmetka>"
          }
        ],
        "type": "invoke"
      },
      {
        "name": "BSSR4.getTblNalogs",
        "parameters": [],
        "then": [
          {
            "name": "getTblNalogsFortblNalogIDResult",
            "type": "set",
            "value": "${result}",
            "valueType": "IEnumerable<Bssr.Models.Bssr4.TblNalog>"
          }
        ],
        "type": "invoke"
      }
    ]
  },
  "name": "Edit Tbl Smetka Stavki",
  "layout": "Main"
}