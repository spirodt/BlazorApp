{
  "components": [
    {
      "type": "content",
      "container": "main",
      "components": [
        {
          "type": "row",
          "components": [
            {
              "type": "col",
              "class": "col-md-12",
              "components": [
                {
                  "type": "template-form",
                  "name": "form0",
                  "components": [
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "User Name",
                              "component": "UserName",
                              "name": "UserNameLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "string",
                              "maxLength": 25,
                              "type": "textbox",
                              "name": "UserName",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${tbluser.UserName}"
                            },
                            {
                              "type": "required-validator",
                              "name": "UserNameRequiredValidator",
                              "component": "UserName",
                              "text": "UserName is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Password",
                              "component": "Password",
                              "name": "PasswordLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "string",
                              "maxLength": 25,
                              "type": "textbox",
                              "name": "Password",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${tbluser.Password}"
                            },
                            {
                              "type": "required-validator",
                              "name": "PasswordRequiredValidator",
                              "component": "Password",
                              "text": "Password is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Full Name",
                              "component": "FullName",
                              "name": "FullNameLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "string",
                              "maxLength": 50,
                              "type": "textbox",
                              "name": "FullName",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${tbluser.FullName}"
                            },
                            {
                              "type": "required-validator",
                              "name": "FullNameRequiredValidator",
                              "component": "FullName",
                              "text": "FullName is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Nivo Na Administracija",
                              "component": "NivoNaAdministracija",
                              "name": "NivoNaAdministracijaLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "int",
                              "type": "numeric",
                              "name": "NivoNaAdministracija",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${tbluser.NivoNaAdministracija}"
                            },
                            {
                              "type": "required-validator",
                              "name": "NivoNaAdministracijaRequiredValidator",
                              "component": "NivoNaAdministracija",
                              "text": "NivoNaAdministracija is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Status",
                              "component": "Status",
                              "name": "StatusLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "int?",
                              "type": "numeric",
                              "name": "Status",
                              "style": {
                                "width": "100%"
                              },
                              "value": "${tbluser.Status}"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Firma Id",
                              "component": "FirmaID",
                              "name": "FirmaIDLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "int",
                              "type": "numeric",
                              "name": "FirmaID",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${tbluser.FirmaID}"
                            },
                            {
                              "type": "required-validator",
                              "name": "FirmaIDRequiredValidator",
                              "component": "FirmaID",
                              "text": "FirmaID is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Tbl Objekt Id",
                              "component": "tblObjektId",
                              "name": "tblObjektIdLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "int?",
                              "type": "numeric",
                              "name": "tblObjektId",
                              "style": {
                                "width": "100%"
                              },
                              "value": "${tbluser.tblObjektId}"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "components": [
                        {
                          "type": "col",
                          "class": "col offset-sm-3",
                          "components": [
                            {
                              "type": "button",
                              "buttonType": "submit",
                              "icon": "save",
                              "text": "Save",
                              "buttonStyle": "primary"
                            },
                            {
                              "type": "button",
                              "buttonStyle": "light",
                              "style": {
                                "marginLeft": "1rem"
                              },
                              "text": "Cancel",
                              "events": {
                                "click": [
                                  {
                                    "type": "close-popup"
                                  }
                                ]
                              }
                            }
                          ]
                        }
                      ]
                    }
                  ],
                  "events": {
                    "submit": [
                      {
                        "error": [
                          {
                            "detail": "Unable to update TblUser",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "BSSR4.updateTblUser",
                        "parameters": [
                          {
                            "name": "ID",
                            "value": "${parameters.ID}"
                          },
                          {
                            "name": "TblUser",
                            "value": "${tbluser}"
                          }
                        ],
                        "then": [
                          {
                            "type": "close-popup",
                            "result": "${tbluser}"
                          }
                        ],
                        "type": "invoke"
                      }
                    ]
                  },
                  "itemType": "Bssr.Models.Bssr4.TblUser",
                  "data": "${tbluser}",
                  "visible": "${tbluser != null}"
                }
              ]
            }
          ]
        }
      ]
    }
  ],
  "events": {
    "load": [
      {
        "name": "BSSR4.getTblUserByID",
        "parameters": [
          {
            "name": "ID",
            "value": "${parameters.ID}"
          }
        ],
        "then": [
          {
            "name": "tbluser",
            "type": "set",
            "value": "${result}",
            "valueType": "Bssr.Models.Bssr4.TblUser"
          }
        ],
        "error": [],
        "type": "invoke"
      }
    ]
  },
  "name": "Edit Tbl Users",
  "layout": "Main"
}