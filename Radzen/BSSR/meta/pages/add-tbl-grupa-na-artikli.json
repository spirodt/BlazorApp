{
  "components": [
    {
      "type": "content",
      "container": "main",
      "components": [
        {
          "type": "row",
          "components": [
            {
              "type": "col",
              "class": "col-md-12",
              "components": [
                {
                  "type": "template-form",
                  "name": "form0",
                  "components": [
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Naziv",
                              "component": "Naziv",
                              "name": "NazivLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "string",
                              "maxLength": 50,
                              "type": "textbox",
                              "name": "Naziv",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${tblgrupanaartikli.Naziv}"
                            },
                            {
                              "type": "required-validator",
                              "name": "NazivRequiredValidator",
                              "component": "Naziv",
                              "text": "Naziv is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Sifra",
                              "component": "Sifra",
                              "name": "SifraLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "string",
                              "maxLength": 20,
                              "type": "textbox",
                              "name": "Sifra",
                              "style": {
                                "width": "100%"
                              },
                              "value": "${tblgrupanaartikli.Sifra}"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Status",
                              "component": "Status",
                              "name": "StatusLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "int?",
                              "type": "numeric",
                              "name": "Status",
                              "style": {
                                "width": "100%"
                              },
                              "value": "${tblgrupanaartikli.Status}"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Tbl Firma",
                              "component": "FirmaID",
                              "name": "FirmaIDLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "data": "${getTblFirmasForFirmaIDResult}",
                              "textProperty": "ImeNaFirma",
                              "valueProperty": "ID",
                              "placeholder": "Choose TblFirma",
                              "valueType": "int",
                              "showSearch": false,
                              "type": "dropdown-datagrid",
                              "name": "FirmaID",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${tblgrupanaartikli.FirmaID}"
                            },
                            {
                              "type": "required-validator",
                              "name": "FirmaIDRequiredValidator",
                              "component": "FirmaID",
                              "text": "FirmaID is required",
                              "style": {
                                "position": "absolute"
                              },
                              "defaultValue": "0"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Image",
                              "component": "Image",
                              "name": "ImageLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "string",
                              "maxLength": 250,
                              "type": "textbox",
                              "name": "Image",
                              "style": {
                                "width": "100%"
                              },
                              "value": "${tblgrupanaartikli.Image}"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "components": [
                        {
                          "type": "col",
                          "class": "col offset-sm-3",
                          "components": [
                            {
                              "type": "button",
                              "buttonType": "submit",
                              "icon": "save",
                              "text": "Save",
                              "buttonStyle": "primary"
                            },
                            {
                              "type": "button",
                              "buttonStyle": "light",
                              "style": {
                                "marginLeft": "1rem"
                              },
                              "text": "Cancel",
                              "events": {
                                "click": [
                                  {
                                    "type": "close-popup"
                                  }
                                ]
                              }
                            }
                          ]
                        }
                      ]
                    }
                  ],
                  "events": {
                    "submit": [
                      {
                        "error": [
                          {
                            "detail": "Unable to create new TblGrupaNaArtikli!",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "BSSR4.createTblGrupaNaArtikli",
                        "parameters": [
                          {
                            "name": "TblGrupaNaArtikli",
                            "value": "${tblgrupanaartikli}"
                          }
                        ],
                        "then": [
                          {
                            "type": "close-popup",
                            "result": "${tblgrupanaartikli}"
                          }
                        ],
                        "type": "invoke"
                      }
                    ]
                  },
                  "data": "${tblgrupanaartikli}",
                  "visible": "${tblgrupanaartikli != null}",
                  "itemType": "Bssr.Models.Bssr4.TblGrupaNaArtikli"
                }
              ]
            }
          ]
        }
      ]
    }
  ],
  "events": {
    "load": [
      {
        "name": "BSSR4.getTblFirmas",
        "parameters": [],
        "then": [
          {
            "name": "getTblFirmasForFirmaIDResult",
            "type": "set",
            "value": "${result}",
            "valueType": "IEnumerable<Bssr.Models.Bssr4.TblFirma>"
          }
        ],
        "type": "invoke"
      },
      {
        "name": "tblgrupanaartikli",
        "type": "set",
        "value": "new Bssr.Models.Bssr4.TblGrupaNaArtikli(){}",
        "valueType": "Bssr.Models.Bssr4.TblGrupaNaArtikli"
      }
    ]
  },
  "name": "Add Tbl Grupa Na Artikli",
  "layout": "Main"
}