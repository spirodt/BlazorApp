{
  "components": [
    {
      "type": "content",
      "container": "main",
      "components": [
        {
          "type": "row",
          "components": [
            {
              "type": "col",
              "class": "col-md-12",
              "components": [
                {
                  "type": "template-form",
                  "name": "form0",
                  "components": [
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Broj",
                              "component": "Broj",
                              "name": "BrojLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "int",
                              "type": "numeric",
                              "name": "Broj",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${tblfiskalnismetki.Broj}"
                            },
                            {
                              "type": "required-validator",
                              "name": "BrojRequiredValidator",
                              "component": "Broj",
                              "text": "Broj is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Datum",
                              "component": "Datum",
                              "name": "DatumLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "DateTime",
                              "dateFormat": "MM/dd/yyyy",
                              "type": "datepicker",
                              "name": "Datum",
                              "style": {
                                "width": "100%"
                              },
                              "value": "${tblfiskalnismetki.Datum}"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Iznos",
                              "component": "Iznos",
                              "name": "IznosLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "decimal",
                              "type": "numeric",
                              "name": "Iznos",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${tblfiskalnismetki.Iznos}"
                            },
                            {
                              "type": "required-validator",
                              "name": "IznosRequiredValidator",
                              "component": "Iznos",
                              "text": "Iznos is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Stornirana",
                              "component": "Stornirana",
                              "name": "StorniranaLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "bool?",
                              "type": "checkbox",
                              "name": "Stornirana",
                              "triState": true,
                              "value": "${tblfiskalnismetki.Stornirana}"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Tbl Smetka",
                              "component": "tblSmetkaID",
                              "name": "tblSmetkaIDLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "data": "${getTblSmetkasFortblSmetkaIDResult}",
                              "textProperty": "Zabeleska",
                              "valueProperty": "ID",
                              "placeholder": "Choose TblSmetka",
                              "valueType": "int",
                              "showSearch": false,
                              "type": "dropdown-datagrid",
                              "name": "tblSmetkaID",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${tblfiskalnismetki.tblSmetkaID}"
                            },
                            {
                              "type": "required-validator",
                              "name": "tblSmetkaIDRequiredValidator",
                              "component": "tblSmetkaID",
                              "text": "tblSmetkaID is required",
                              "style": {
                                "position": "absolute"
                              },
                              "defaultValue": "0"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Firma Id",
                              "component": "FirmaID",
                              "name": "FirmaIDLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "int",
                              "type": "numeric",
                              "name": "FirmaID",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${tblfiskalnismetki.FirmaID}"
                            },
                            {
                              "type": "required-validator",
                              "name": "FirmaIDRequiredValidator",
                              "component": "FirmaID",
                              "text": "FirmaID is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Kod Na Dokument",
                              "component": "KodNaDokument",
                              "name": "KodNaDokumentLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "string",
                              "maxLength": 70,
                              "type": "textbox",
                              "name": "KodNaDokument",
                              "style": {
                                "width": "100%"
                              },
                              "value": "${tblfiskalnismetki.KodNaDokument}"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "components": [
                        {
                          "type": "col",
                          "class": "col offset-sm-3",
                          "components": [
                            {
                              "type": "button",
                              "buttonType": "submit",
                              "icon": "save",
                              "text": "Save",
                              "buttonStyle": "primary"
                            },
                            {
                              "type": "button",
                              "buttonStyle": "light",
                              "style": {
                                "marginLeft": "1rem"
                              },
                              "text": "Cancel",
                              "events": {
                                "click": [
                                  {
                                    "type": "close-popup"
                                  }
                                ]
                              }
                            }
                          ]
                        }
                      ]
                    }
                  ],
                  "events": {
                    "submit": [
                      {
                        "error": [
                          {
                            "detail": "Unable to update TblFiskalniSmetki",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "BSSR4.updateTblFiskalniSmetki",
                        "parameters": [
                          {
                            "name": "ID",
                            "value": "${parameters.ID}"
                          },
                          {
                            "name": "TblFiskalniSmetki",
                            "value": "${tblfiskalnismetki}"
                          }
                        ],
                        "then": [
                          {
                            "type": "close-popup",
                            "result": "${tblfiskalnismetki}"
                          }
                        ],
                        "type": "invoke"
                      }
                    ]
                  },
                  "itemType": "Bssr.Models.Bssr4.TblFiskalniSmetki",
                  "data": "${tblfiskalnismetki}",
                  "visible": "${tblfiskalnismetki != null}"
                }
              ]
            }
          ]
        }
      ]
    }
  ],
  "events": {
    "load": [
      {
        "name": "BSSR4.getTblFiskalniSmetkiByID",
        "parameters": [
          {
            "name": "ID",
            "value": "${parameters.ID}"
          }
        ],
        "then": [
          {
            "name": "tblfiskalnismetki",
            "type": "set",
            "value": "${result}",
            "valueType": "Bssr.Models.Bssr4.TblFiskalniSmetki"
          }
        ],
        "error": [],
        "type": "invoke"
      },
      {
        "name": "BSSR4.getTblSmetkas",
        "parameters": [],
        "then": [
          {
            "name": "getTblSmetkasFortblSmetkaIDResult",
            "type": "set",
            "value": "${result}",
            "valueType": "IEnumerable<Bssr.Models.Bssr4.TblSmetka>"
          }
        ],
        "type": "invoke"
      }
    ]
  },
  "name": "Edit Tbl Fiskalni Smetki",
  "layout": "Main",
  "access": []
}