    }
  ],
  "events": {
    "load": [
      {
        "name": "BSSR4.getTblPrevodiByID",
        "parameters": [
          {
            "name": "ID",
            "value": "${parameters.ID}"
          }
        ],
        "then": [
          {
            "name": "tblprevodi",
            "type": "set",
            "value": "${result}",
            "valueType": "Bssr.Models.Bssr4.TblPrevodi"
          }
        ],
        "error": [],
        "type": "invoke"
      }
    ]
  },
  "name": "Edit Tbl Prevodi",
