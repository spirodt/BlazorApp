{
  "components": [
    {
      "type": "content",
      "container": "main",
      "components": [
        {
          "type": "row",
          "components": [
            {
              "type": "col",
              "class": "col-md-12",
              "components": [
                {
                  "type": "template-form",
                  "name": "form0",
                  "components": [
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Change Number",
                              "component": "change_number",
                              "name": "change_numberLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "int",
                              "type": "numeric",
                              "name": "change_number",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${changelog.change_number}"
                            },
                            {
                              "type": "required-validator",
                              "name": "change_numberRequiredValidator",
                              "component": "change_number",
                              "text": "change_number is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Complete Dt",
                              "component": "complete_dt",
                              "name": "complete_dtLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "DateTime?",
                              "dateFormat": "MM/dd/yyyy",
                              "type": "datepicker",
                              "name": "complete_dt",
                              "style": {
                                "width": "100%"
                              },
                              "value": "${changelog.complete_dt}"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Applied By",
                              "component": "applied_by",
                              "name": "applied_byLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "string",
                              "maxLength": 100,
                              "type": "textbox",
                              "name": "applied_by",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${changelog.applied_by}"
                            },
                            {
                              "type": "required-validator",
                              "name": "applied_byRequiredValidator",
                              "component": "applied_by",
                              "text": "applied_by is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Description",
                              "component": "description",
                              "name": "descriptionLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "string",
                              "maxLength": 500,
                              "type": "textbox",
                              "name": "description",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${changelog.description}"
                            },
                            {
                              "type": "required-validator",
                              "name": "descriptionRequiredValidator",
                              "component": "description",
                              "text": "description is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "components": [
                        {
                          "type": "col",
                          "class": "col offset-sm-3",
                          "components": [
                            {
                              "type": "button",
                              "buttonType": "submit",
                              "icon": "save",
                              "text": "Save",
                              "buttonStyle": "primary"
                            },
                            {
                              "type": "button",
                              "buttonStyle": "light",
                              "style": {
                                "marginLeft": "1rem"
                              },
                              "text": "Cancel",
                              "events": {
                                "click": [
                                  {
                                    "type": "close-popup"
                                  }
                                ]
                              }
                            }
                          ]
                        }
                      ]
                    }
                  ],
                  "events": {
                    "submit": [
                      {
                        "error": [
                          {
                            "detail": "Unable to create new Changelog!",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "BSSR4.createChangelog",
                        "parameters": [
                          {
                            "name": "Changelog",
                            "value": "${changelog}"
                          }
                        ],
                        "then": [
                          {
                            "type": "close-popup",
                            "result": "${changelog}"
                          }
                        ],
                        "type": "invoke"
                      }
                    ]
                  },
                  "data": "${changelog}",
                  "visible": "${changelog != null}",
                  "itemType": "Bssr.Models.Bssr4.Changelog"
                }
              ]
            }
          ]
        }
      ]
    }
  ],
  "events": {
    "load": [
      {
        "name": "changelog",
        "type": "set",
        "value": "new Bssr.Models.Bssr4.Changelog(){}",
        "valueType": "Bssr.Models.Bssr4.Changelog"
      }
    ]
  },
  "name": "Add Changelog",
  "layout": "Main"
}