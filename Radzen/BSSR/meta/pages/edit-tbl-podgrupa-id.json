{
  "components": [
    {
      "type": "content",
      "container": "main",
      "components": [
        {
          "type": "row",
          "components": [
            {
              "type": "col",
              "class": "col-md-12",
              "components": [
                {
                  "type": "template-form",
                  "name": "form0",
                  "components": [
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Tbl Grupa Na Artikli",
                              "component": "GrupaID",
                              "name": "GrupaIDLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "data": "${getTblGrupaNaArtiklisForGrupaIDResult}",
                              "textProperty": "Naziv",
                              "valueProperty": "ID",
                              "placeholder": "Choose TblGrupaNaArtikli",
                              "valueType": "int",
                              "showSearch": false,
                              "type": "dropdown-datagrid",
                              "name": "GrupaID",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${tblpodgrupaid.GrupaID}"
                            },
                            {
                              "type": "required-validator",
                              "name": "GrupaIDRequiredValidator",
                              "component": "GrupaID",
                              "text": "GrupaID is required",
                              "style": {
                                "position": "absolute"
                              },
                              "defaultValue": "0"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Naziv",
                              "component": "Naziv",
                              "name": "NazivLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "string",
                              "maxLength": 50,
                              "type": "textbox",
                              "name": "Naziv",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${tblpodgrupaid.Naziv}"
                            },
                            {
                              "type": "required-validator",
                              "name": "NazivRequiredValidator",
                              "component": "Naziv",
                              "text": "Naziv is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Firma Id",
                              "component": "FirmaID",
                              "name": "FirmaIDLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "int",
                              "type": "numeric",
                              "name": "FirmaID",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${tblpodgrupaid.FirmaID}"
                            },
                            {
                              "type": "required-validator",
                              "name": "FirmaIDRequiredValidator",
                              "component": "FirmaID",
                              "text": "FirmaID is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Status",
                              "component": "Status",
                              "name": "StatusLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "int?",
                              "type": "numeric",
                              "name": "Status",
                              "style": {
                                "width": "100%"
                              },
                              "value": "${tblpodgrupaid.Status}"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Image",
                              "component": "Image",
                              "name": "ImageLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "string",
                              "maxLength": 250,
                              "type": "textbox",
                              "name": "Image",
                              "style": {
                                "width": "100%"
                              },
                              "value": "${tblpodgrupaid.Image}"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "components": [
                        {
                          "type": "col",
                          "class": "col offset-sm-3",
                          "components": [
                            {
                              "type": "button",
                              "buttonType": "submit",
                              "icon": "save",
                              "text": "Save",
                              "buttonStyle": "primary"
                            },
                            {
                              "type": "button",
                              "buttonStyle": "light",
                              "style": {
                                "marginLeft": "1rem"
                              },
                              "text": "Cancel",
                              "events": {
                                "click": [
                                  {
                                    "type": "close-popup"
                                  }
                                ]
                              }
                            }
                          ]
                        }
                      ]
                    }
                  ],
                  "events": {
                    "submit": [
                      {
                        "error": [
                          {
                            "detail": "Unable to update TblPodgrupaId",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "BSSR4.updateTblPodgrupaId",
                        "parameters": [
                          {
                            "name": "ID",
                            "value": "${parameters.ID}"
                          },
                          {
                            "name": "TblPodgrupaId",
                            "value": "${tblpodgrupaid}"
                          }
                        ],
                        "then": [
                          {
                            "type": "close-popup",
                            "result": "${tblpodgrupaid}"
                          }
                        ],
                        "type": "invoke"
                      }
                    ]
                  },
                  "itemType": "Bssr.Models.Bssr4.TblPodgrupaId",
                  "data": "${tblpodgrupaid}",
                  "visible": "${tblpodgrupaid != null}"
                }
              ]
            }
          ]
        }
      ]
    }
  ],
  "events": {
    "load": [
      {
        "name": "BSSR4.getTblPodgrupaIdByID",
        "parameters": [
          {
            "name": "ID",
            "value": "${parameters.ID}"
          }
        ],
        "then": [
          {
            "name": "tblpodgrupaid",
            "type": "set",
            "value": "${result}",
            "valueType": "Bssr.Models.Bssr4.TblPodgrupaId"
          }
        ],
        "error": [],
        "type": "invoke"
      },
      {
        "name": "BSSR4.getTblGrupaNaArtiklis",
        "parameters": [],
        "then": [
          {
            "name": "getTblGrupaNaArtiklisForGrupaIDResult",
            "type": "set",
            "value": "${result}",
            "valueType": "IEnumerable<Bssr.Models.Bssr4.TblGrupaNaArtikli>"
          }
        ],
        "type": "invoke"
      }
    ]
  },
  "name": "Edit Tbl Podgrupa ID",
  "layout": "Main"
}