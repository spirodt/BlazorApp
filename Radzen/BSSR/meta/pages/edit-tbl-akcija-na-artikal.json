{
  "components": [
    {
      "type": "content",
      "container": "main",
      "components": [
        {
          "type": "row",
          "components": [
            {
              "type": "col",
              "class": "col-md-12",
              "components": [
                {
                  "type": "template-form",
                  "name": "form0",
                  "components": [
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Datum Pocetok",
                              "component": "DatumPocetok",
                              "name": "DatumPocetokLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "DateTime",
                              "dateFormat": "MM/dd/yyyy",
                              "type": "datepicker",
                              "name": "DatumPocetok",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${tblakcijanaartikal.DatumPocetok}"
                            },
                            {
                              "type": "required-validator",
                              "name": "DatumPocetokRequiredValidator",
                              "component": "DatumPocetok",
                              "text": "DatumPocetok is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Datum Kraj",
                              "component": "DatumKraj",
                              "name": "DatumKrajLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "DateTime",
                              "dateFormat": "MM/dd/yyyy",
                              "type": "datepicker",
                              "name": "DatumKraj",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${tblakcijanaartikal.DatumKraj}"
                            },
                            {
                              "type": "required-validator",
                              "name": "DatumKrajRequiredValidator",
                              "component": "DatumKraj",
                              "text": "DatumKraj is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Tbl Artikal",
                              "component": "tblArtikalID",
                              "name": "tblArtikalIDLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "data": "${getTblArtikalsFortblArtikalIDResult}",
                              "textProperty": "Sifra",
                              "valueProperty": "ID",
                              "placeholder": "Choose TblArtikal",
                              "valueType": "int",
                              "showSearch": false,
                              "type": "dropdown-datagrid",
                              "name": "tblArtikalID",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${tblakcijanaartikal.tblArtikalID}"
                            },
                            {
                              "type": "required-validator",
                              "name": "tblArtikalIDRequiredValidator",
                              "component": "tblArtikalID",
                              "text": "tblArtikalID is required",
                              "style": {
                                "position": "absolute"
                              },
                              "defaultValue": "0"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Akciska Cena",
                              "component": "AkciskaCena",
                              "name": "AkciskaCenaLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "decimal",
                              "type": "numeric",
                              "name": "AkciskaCena",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${tblakcijanaartikal.AkciskaCena}"
                            },
                            {
                              "type": "required-validator",
                              "name": "AkciskaCenaRequiredValidator",
                              "component": "AkciskaCena",
                              "text": "AkciskaCena is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Rabat",
                              "component": "Rabat",
                              "name": "RabatLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "decimal?",
                              "type": "numeric",
                              "name": "Rabat",
                              "style": {
                                "width": "100%"
                              },
                              "value": "${tblakcijanaartikal.Rabat}"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "components": [
                        {
                          "type": "col",
                          "class": "col offset-sm-3",
                          "components": [
                            {
                              "type": "button",
                              "buttonType": "submit",
                              "icon": "save",
                              "text": "Save",
                              "buttonStyle": "primary"
                            },
                            {
                              "type": "button",
                              "buttonStyle": "light",
                              "style": {
                                "marginLeft": "1rem"
                              },
                              "text": "Cancel",
                              "events": {
                                "click": [
                                  {
                                    "type": "close-popup"
                                  }
                                ]
                              }
                            }
                          ]
                        }
                      ]
                    }
                  ],
                  "events": {
                    "submit": [
                      {
                        "error": [
                          {
                            "detail": "Unable to update TblAkcijaNaArtikal",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "BSSR4.updateTblAkcijaNaArtikal",
                        "parameters": [
                          {
                            "name": "ID",
                            "value": "${parameters.ID}"
                          },
                          {
                            "name": "TblAkcijaNaArtikal",
                            "value": "${tblakcijanaartikal}"
                          }
                        ],
                        "then": [
                          {
                            "type": "close-popup",
                            "result": "${tblakcijanaartikal}"
                          }
                        ],
                        "type": "invoke"
                      }
                    ]
                  },
                  "itemType": "Bssr.Models.Bssr4.TblAkcijaNaArtikal",
                  "data": "${tblakcijanaartikal}",
                  "visible": "${tblakcijanaartikal != null}"
                }
              ]
            }
          ]
        }
      ]
    }
  ],
  "events": {
    "load": [
      {
        "name": "BSSR4.getTblAkcijaNaArtikalByID",
        "parameters": [
          {
            "name": "ID",
            "value": "${parameters.ID}"
          }
        ],
        "then": [
          {
            "name": "tblakcijanaartikal",
            "type": "set",
            "value": "${result}",
            "valueType": "Bssr.Models.Bssr4.TblAkcijaNaArtikal"
          }
        ],
        "error": [],
        "type": "invoke"
      },
      {
        "name": "BSSR4.getTblArtikals",
        "parameters": [],
        "then": [
          {
            "name": "getTblArtikalsFortblArtikalIDResult",
            "type": "set",
            "value": "${result}",
            "valueType": "IEnumerable<Bssr.Models.Bssr4.TblArtikal>"
          }
        ],
        "type": "invoke"
      }
    ]
  },
  "name": "Edit Tbl Akcija Na Artikal",
  "layout": "Main",
  "access": []
}